view=flowmap
view.flowmap.colorScheme=Dark
view.flowmap.fillEdgesWithGradient=true
view.flowmap.showDirectionMarkers=false
view.flowmap.showNodes=true
view.flowmap.edgeWidth=1
view.flowmap.edgeOpacity=105
view.flowmap.edgeBundling.updateViewAfterEachStep=false
view.flowmap.edgeBundling.numberOfCycles=10
view.flowmap.edgeBundling.edgeStiffness=0.1
view.flowmap.edgeBundling.compatibilityThreshold=0.4
view.flowmap.edgeBundling.stepSize=4
view.flowmap.edgeBundling.stepDampingFactor=0.2
view.flowmap.edgeBundling.stepsIn1stCycle=50
view.flowmap.edgeBundling.simpleCompatibilityMeasure=true

window.size=1024x768
window.settings.show=true
window.settings.activeTab=Edge bundling

data=csv
data.csv.separator=,
data.csv.charset=utf-8
data.csv.nodes.src=../data/test/bundling/500flows/nodes.csv
data.csv.flows.src=../data/test/bundling/500flows/flows.csv

data.attrs.node.id=id
data.attrs.node.label=name
data.attrs.node.lat=lat
data.attrs.node.lon=lon
data.attrs.flow.origin=src
data.attrs.flow.dest=dest
data.attrs.flow.weight.csvList=val

map.projection=None
